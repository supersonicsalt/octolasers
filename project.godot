; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="octolasers"
config/version="0.0.menuUpdate.1"
run/main_scene="res://scenes/menu.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/size/mode=2
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"

[internationalization]

locale/translation_remaps={}
locale/translations=PackedStringArray("res://translations/mainTranslation.en.translation", "res://translations/mainTranslation.short.translation", "res://translations/mainTranslation.he.translation")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
textures/default_filters/use_nearest_mipmap_filter=true
